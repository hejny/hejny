        "destroyable": "0.12.0",
        "eslint": "8.31.0",
        "eslint-config-next": "13.1.1",
        "gh-pages": "4.0.0",
        "glob-promise": "6.0.1",
        "isomorphic-fetch": "3.0.0",
        "jest": "29.4.0",
        "jest-environment-jsdom": "29.4.0",
        "localtunnel": "2.0.2",
        "lodash": "4.17.21",
