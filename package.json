    "description": "Utilize edge new exponential technoligies in your project and business",
    "homepage": "https://prototyping.hejny.org/",
    "repository": {
        "type": "git",
        "url": "https://github.com/hejny/rapid-prototyping-wizard"
    },
    "scripts": {
        "dev": "next dev -p 4455",
        "build": "rm -rf ./build && next build && next export -o build && next-export-optimize-images && echo prototyping.hejny.org > ./build/CNAME && echo > ./build/.nojekyll",
        "start": "next start",
        "lint": "next lint",
        "test": "npm run test-unit && npm run test-integration",
        "test-unit": "jest",
        "test-integration": "ts-node ./scripts/integration-test-with-server/integration-test-with-server.ts",
        "test-integration-without-server": "./node_modules/.bin/cypress run",
        "test-integration-interactive": "./node_modules/.bin/cypress open",
        "test-with-lighthouse": " lighthouse https://collboard.com/ --output html --output-path ./other/lighthouse-reports/report.html --view",
        "prettier": "prettier --config .prettierrc --ignore-unknown --write \"{pages,public,styles}/**/*\"",
        "prettier-commit": "git diff --exit-code && npm run prettier && git add . && git commit --allow-empty -m \"ðŸ§¹ Prettier\"",
        "preversion": "git pull && npm run prettier-commit && npm run lint && npm test",
        "postversion": "git push && git push --tags && echo \"Check the deploy process: https://github.com/hejny/rapid-prototyping-wizard/actions/\""
    },
    "devDependencies": {
        "@next/font": "13.1.1",
